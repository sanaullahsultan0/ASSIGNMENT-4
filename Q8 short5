#include <iostream>
#include <vector>
using namespace std;

int main() {
    int N, target, count = 0;
    cin >> N;

    vector<int> v(N);
    for (int i = 0; i < N; i++)
        cin >> v[i];

    cin >> target;

    for (int x : v)
        if (x == target)
            count++;

    cout << count;
    return 0;
}