#include <iostream>
#include <vector>
using namespace std;

void reverseVector(vector<int>& v) {
    int i = 0, j = v.size() - 1;
    while (i < j) {
        swap(v[i], v[j]);
        i++;
        j--;
    }
}

int main() {
    vector<int> v = {1, 2, 3, 4, 5};
    reverseVector(v);

    for (int x : v)
        cout << x << " ";

    return 0;
}