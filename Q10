#include <iostream>
using namespace std;

class MyVector {
private:
    int* arr;
    int capacity;
    int length;

public:
    MyVector() {
        capacity = 10;
        arr = new int[capacity];
        length = 0;
    }

    void push_back(int x) {
        if (length == capacity) {
            int* temp = new int[2 * capacity];
            for (int i = 0; i < length; i++)
                temp[i] = arr[i];
            delete[] arr;
            arr = temp;
            capacity *= 2;
        }
        arr[length++] = x;
    }

    void pop_back() {
        if (length > 0)
            length--;
    }

    int size() {
        return length;
    }

    int get(int index) {
        if (index >= 0 && index < length)
            return arr[index];
        return -1;
    }

    ~MyVector() {
        delete[] arr;
    }
};

int main() {
    MyVector v;

    v.push_back(10);
    v.push_back(20);
    v.push_back(30);

    cout << "Size: " << v.size() << endl;

    for (int i = 0; i < v.size(); i++)
        cout << v.get(i) << " ";
    cout << endl;

    v.pop_back();
    cout << "Size after pop: " << v.size() << endl;

    for (int i = 0; i < v.size(); i++)
        cout << v.get(i) << " ";
    cout << endl;

    return 0;
}