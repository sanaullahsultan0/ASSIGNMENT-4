#include <iostream>
#include <vector>
using namespace std;

int main() {
    int N;
    cin >> N;

    vector<int> v(N);
    for (int i = 0; i < N; i++)
        cin >> v[i];

    int max = v[0], min = v[0];

    for (int i = 1; i < N; i++) {
        if (v[i] > max) max = v[i];
        if (v[i] < min) min = v[i];
    }

    cout << max << " " << min;
    return 0;
}