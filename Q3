#include <iostream>
using namespace std;

int main() {
    int seat[10][10] = {0};
    int ch, r, c;

    do {
        cin >> ch;

        if (ch == 1) {
            for (int i = 0; i < 10; i++) {
                for (int j = 0; j < 10; j++) {
                    cout << seat[i][j] << " ";
                }
                cout << endl;
            }
        }

        else if (ch == 2) {
            cin >> r >> c;
            if (seat[r][c] == 0)
                seat[r][c] = 1;
        }

        else if (ch == 3) {
            cin >> r >> c;
            if (seat[r][c] == 1)
                seat[r][c] = 0;
        }

        else if (ch == 4) {
            int full = 0, empty = 0, partial = 0;

            for (int i = 0; i < 10; i++) {
                int count = 0;
                for (int j = 0; j < 10; j++) {
                    if (seat[i][j] == 1)
                        count++;
                }
                if (count == 10)
                    full++;
                else if (count == 0)
                    empty++;
                else
                    partial++;
            }

            cout << full << " " << partial << " " << empty << endl;
        }

    } while (ch != 5);

    return 0;
}
