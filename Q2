#include <iostream>
using namespace std;

int main() {
    int sales[5][7];
    int total[5] = {0};

    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 7; j++) {
            cin >> sales[i][j];
            total[i] = total[i] + sales[i][j];
        }
    }

    int maxProduct = 0;
    for (int i = 1; i < 5; i++) {
        if (total[i] > total[maxProduct]) {
            maxProduct = i;
        }
    }

    int bestDay = 0, maxSales = 0;
    for (int j = 0; j < 7; j++) {
        int dayTotal = 0;
        for (int i = 0; i < 5; i++) {
            dayTotal = dayTotal + sales[i][j];
        }
        if (dayTotal > maxSales) {
            maxSales = dayTotal;
            bestDay = j;
        }
    }

    cout << "Highest Product: " << maxProduct + 1 << endl;
    cout << "Best Day: " << bestDay + 1 << endl;

    return 0;
}
