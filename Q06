#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;

    int mat[10][10];
    int arr[100];
    int k = 0;

    for(int i = 0; i < N; i++) {
        for(int j = 0; j < N; j++) {
            cin >> mat[i][j];
            arr[k++] = mat[i][j];
        }
    }

    for(int i = 0; i < k - 1; i++) {
        for(int j = i + 1; j < k; j++) {
            if(arr[i] > arr[j]) {
                int temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }

    k = 0;
    for(int i = 0; i < N; i++) {
        for(int j = 0; j < N; j++) {
            mat[i][j] = arr[k++];
        }
    }

    for(int i = 0; i < N; i++) {
        for(int j = 0; j < N; j++) {
            cout << mat[i][j] << " ";
        }
        cout << endl;
    }

    return 0;
}

